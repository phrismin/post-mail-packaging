<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.coreservice.domain">
    <select id="getTemplateById" resultType="string">
        select template
        from templates
        where id = #{id}
    </select>
</mapper>